//TO FIND SECOND LARGEST ELEMENT IN THE ARRAY//
#include <stdio.h>
int main() {
    int a[] = {14, 45, 88, 32, 92};
    int n = 5, first, second;
    first = second = -1;
    for (int i = 0; i < n; i++) {
        if (a[i] > first) {
            first = a[i];
        }
}
    for (int i = 0; i < n; i++) {
        if (a[i] > second && a[i] < first) {
            second = a[i];
        }
    }
    if (second == -1) {
        printf("The second largest element does not exist");
    } else {
        printf("The second largest element is %d", second);
    }

    return 0;
}
